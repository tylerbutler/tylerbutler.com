{
  "name": "tylerbutler.com",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://github.com/tylerbutler/tylerbutler.com-hugo#readme",
  "bugs": {
    "url": "https://github.com/tylerbutler/tylerbutler.com-hugo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylerbutler/tylerbutler.com-hugo.git"
  },
  "license": "MIT",
  "author": "Tyler Butler <tyler@tylerbutler.com>",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "baseline": "npm run baseline:build && npm run baseline:lighthouse && npm run baseline:accessibility && npm run baseline:bundle && npm run baseline:images",
    "baseline:accessibility": "npm run test:accessibility",
    "baseline:build": "npm run build",
    "baseline:bundle": "npm run test:bundle",
    "baseline:images": "npm run test:images",
    "baseline:lighthouse": "npm run test:lighthouse && npm run test:lighthouse:mobile",
    "baseline:web-vitals": "npm run test:web-vitals",
    "build": "cross-env GITHUB_REPO_OWNER=tylerbutler GITHUB_REPO_NAME=private-resources astro build",
    "check": "astro check --minimumSeverity warning",
    "dev": "astro dev",
    "format": "biome check --write --linter-enabled=false",
    "install:python": "pip install -r requirements.txt",
    "linkcheck": "start-server-and-test dev http://localhost:4321 'cat complete-urls.txt | xargs lychee'",
    "optimize:fonts": "node scripts/optimize-fonts.mjs",
    "preview": "astro preview",
    "test": "npm run test:visual && npm run test:performance && npm run test:accessibility && npm run test:bundle",
    "test:accessibility": "pa11y-ci",
    "test:bundle": "NODE_ENV=production npm run build && echo \"ðŸ“Š Bundle analysis generated at dist/bundle-analysis.html\"",
    "test:headed": "playwright test --headed",
    "test:images": "node scripts/check-images.js",
    "test:lighthouse": "lhci autorun --config=lighthouserc.cjs",
    "test:lighthouse:mobile": "lhci autorun --config=lighthouserc.mobile.cjs",
    "test:performance": "npm run test:lighthouse && npm run test:web-vitals",
    "test:update": "playwright test --update-snapshots",
    "test:visual": "playwright test tests/visual",
    "test:web-vitals": "playwright test scripts/web-vitals-test.js"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/netlify": "^6.6.3",
    "@astrojs/rss": "^4.0.14",
    "@astrojs/sitemap": "^3.6.0",
    "@expressive-code/plugin-line-numbers": "^0.41.3",
    "@floating-ui/dom": "^1.7.4",
    "@fontsource/lato": "^5.2.7",
    "@lucide/astro": "^0.555.0",
    "@netlify/api": "^14.0.11",
    "astro": "^5.16.3",
    "astro-broken-links-checker": "github:imazen/astro-broken-link-checker",
    "astro-expressive-code": "^0.41.3",
    "astro-toc": "^0.2.0",
    "expressive-code-twoslash": "^0.5.3",
    "littlefoot": "^4.1.3",
    "pathe": "^2.0.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-expressive-code": "^0.41.3",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-github-blockquote-alert": "^2.0.0",
    "remark-mermaid": "^0.2.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remark-shift-headings": "^0.1.0",
    "remark-smartypants": "^3.0.2",
    "remark-stringify": "^11.0.0",
    "sanitize-html": "^2.17.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.57.0",
    "cross-env": "^10.1.0",
    "pa11y-ci": "^4.0.1",
    "pagefind": "^1.4.0",
    "postcss-nesting": "^13.0.2",
    "rehype-footnotes": "^0.1.0",
    "remark-lazy-links": "^0.1.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
